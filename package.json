{
	"name": "daikin",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"db:create": "wrangler d1 create daikin",
		"db:migrate": "wrangler d1 execute daikin --file=./schema.sql",
		"db:migrate:local": "wrangler d1 execute daikin --local --file=./schema.sql",
		"deploy": "wrangler pages deploy .svelte-kit/cloudflare",
		"cf:dev": "wrangler pages dev .svelte-kit/cloudflare --d1=DB"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.17",
		"@types/better-sqlite3": "^7.6.13",
		"better-sqlite3": "^12.5.0",
		"daisyui": "^5.5.5",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"vite": "^7.2.2",
		"wrangler": "^4.51.0"
	},
	"dependencies": {
		"better-auth": "^1.4.3",
		"chart.js": "^4.5.1",
		"kysely": "^0.28.8",
		"kysely-d1": "^0.4.0"
	}
}
